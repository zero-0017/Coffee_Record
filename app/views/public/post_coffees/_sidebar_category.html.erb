<table class="table">
  <thead>
    <tr>
      <th class="text-center">カテゴリー検索</th>
    </tr>
  </thead>

  <tbody>
    <% categorys.each do |category| %>
      <tr>
        <td>
          <%= link_to category.category_name, category_path(category.id), class:"text-dark"%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>